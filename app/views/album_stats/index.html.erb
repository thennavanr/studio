<p id="notice"><%= notice %></p>

<h1>Listing Album Stats</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @album_stats.each do |album_stat| %>
      <tr>
        <td><%= link_to 'Show', album_stat %></td>
        <td><%= link_to 'Edit', edit_album_stat_path(album_stat) %></td>
        <td><%= link_to 'Destroy', album_stat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Album stat', new_album_stat_path %>
